Процедура СА_ПроверкаДобавленияДокумента(СтруктураПроверки)
	Запрос = Новый Запрос;
	Запрос.Текст =  "
	|ВЫБРАТЬ
	|ОК.Ссылка КАК Существует
	|ИЗ Документ.ОтчетКомиссионера КАК ОК
	|ГДЕ ОК.Проведен
	|";
	Для каждого ст ИЗ СтруктураПроверки Цикл
		Запрос.Текст = Запрос.Текст + " И ОК."+Строка(ст.Ключ)+"= &"+Строка(ст.Ключ);
	КонецЦикла;
	
	Для каждого ст ИЗ СтруктураПроверки Цикл
		Запрос.УстановитьПараметр(Строка(ст.Ключ),ст.Значение);
	КонецЦикла;
    Результат = Запрос.Выполнить().Выгрузить();
	Если НЕ Результат.Количество()=0 Тогда
		СтруктураПроверки.Вставить("Существует",Неопределено) ;
		Для каждого стр из Результат Цикл
			СК = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(стр);
			ЗаполнитьЗначенияСвойств(СтруктураПроверки,СК);
		КонецЦикла;
	КонецЕсли;
КонецПроцедуры
