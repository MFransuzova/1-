Процедура АИГ_ЗаполнитьТаблицуПроводок()
	
	Запрос = Новый Запрос;
	Запрос.Текст = "
	|ВЫБРАТЬ
	|
	|ДвиженияССубконто.Период,
	|ДвиженияССубконто.Регистратор,
	|ДвиженияССубконто.Номерстроки,
	|ДвиженияССубконто.СчетДт,
	|ДвиженияССубконто.СубконтоДт1,
	|ДвиженияССубконто.ВидСубконтоДт1,
	|ДвиженияССубконто.СубконтоДт2,
	|ДвиженияССубконто.ВидСубконтоДт2,
	|ДвиженияССубконто.СубконтоДт3,
	|ДвиженияССубконто.ВидСубконтоДт3,
	|ДвиженияССубконто.СубконтоДт4,
	|ДвиженияССубконто.ВидСубконтоДт4,
	|ДвиженияССубконто.СчетКт,
	|ДвиженияССубконто.СубконтоКт1,
	|ДвиженияССубконто.ВидСубконтоКт1,
	|ДвиженияССубконто.СубконтоКт2,
	|ДвиженияССубконто.ВидСубконтоКт2,
	|ДвиженияССубконто.СубконтоКт3,
	|ДвиженияССубконто.ВидСубконтоКт3,
	|ДвиженияССубконто.СубконтоКт4,
	|ДвиженияССубконто.ВидСубконтоКт4,
	|ДвиженияССубконто.ВалютаДт,
	|ДвиженияССубконто.ВалютаКт,
	|ДвиженияССубконто.ПодразделениеМСФО,
	|ДвиженияССубконто.ВидДокумента,
	|ДвиженияССубконто.Сумма КАК СуммаПроводки,
	|ДвиженияССубконто.ВалютнаяСуммаДт,
	|ДвиженияССубконто.ВалютнаяСуммаКт,
	|ДвиженияССубконто.КоличествоДт,
	|ДвиженияССубконто.КоличествоКт,
	|ДвиженияССубконто.Содержание,
	|ДвиженияССубконто.НомерЖурнала
	|
	|ИЗ РегистрБухгалтерии.КК_Международный.ДвиженияССубконто(&НачДата, &КонДата, Регистратор = &НужныйДокумент И Активность,,) КАК ДвиженияССубконто
	
	|";
	Запрос.УстановитьПараметр("НачДата",НачалоДня(Объект.Дата));
	Запрос.УстановитьПараметр("КонДата",КонецДня(Объект.Дата));
	Запрос.УстановитьПараметр("НужныйДокумент",Объект.Ссылка);
	Результат = Запрос.Выполнить().Выгрузить();        
	СтруктураПроводок = Новый Структура;
	ПроводкиДокумента.Очистить();

	Если НЕ Результат.Количество() = 0 Тогда  
		СтруктураПроводок.Вставить("ТабПроводок",Результат);
	КонецЕсли;
	Если СтруктураПроводок.Свойство("ТабПроводок") Тогда
		Для каждого стр из СтруктураПроводок.ТабПроводок Цикл
			СК = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(стр);
			нс = ПроводкиДокумента.Добавить();
			ЗаполнитьЗначенияСвойств(нс,СК);
		КонецЦикла;	
	КонецЕсли;

	
КонецПроцедуры

// - АИГ Французова 27.12.2021 (Вывод проводок документов ОперацияМеждународная)
