				СтруктураЗапроса = Новый Структура;
				СтруктураЗапроса.Вставить("Подписант","");        
				СтруктураЗапроса.Вставить("ПодписантДолжность", "руководитель");  
				СтруктураЗапроса.Вставить("Организация",ОрганизацияГруппировка.Организация);
				СтруктураЗапроса.Вставить("Склад",СкладГруппировка.Склад);
				СтруктураЗапроса.Вставить("ДатаДокумента",ДатаОкончания); 
				ДБ_ДанныеПодписанта(СтруктураЗапроса);
				ТекстОрганизации = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
				НСтр("ru= 'УТВЕРЖДАЮ 
				|%1
				|%2
				|__________________ %3
				| %4'"), 
				СтруктураЗапроса.ПодписантДолжность,
				Строка(ОрганизацияГруппировка.Организация),
				СтруктураЗапроса.Подписант,
				Формат(ДатаОкончания, "ДЛФ=ДД"));  
				
				ОбластьЗаголовок.Параметры.Организация = ТекстОрганизации;  
