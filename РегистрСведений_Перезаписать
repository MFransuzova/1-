Процедура УстановитьСервер() 
	ЗАпрос = Новый Запрос;
	Запрос.Текст = " 
	|ВЫБРАТЬ  
	//ПЕРВЫЕ 20     
	|Рег.Период,
	|Рег.ДокументСсылка,
	|Рег.УдалитьСтатус,
	|Рег.УдалитьСтатус КАК Статус, 
	|Рег.Ответственный
	|ИЗ РегистрСведений.ПО_ИсторияИзмененияСтатусов КАК Рег  
	|ГДЕ НЕ  Рег.УдалитьСтатус = Рег.Статус
	|";        
	
	Результат = Запрос.Выполнить().Выгрузить();
	//ПечатьТЗ1(Результат,"","");   
	//Возврат;
	Для каждого стр из Результат Цикл 
		Отбор = Новый Структура;
		Отбор.Вставить("ДокументСсылка",стр.ДокументСсылка);
		Отбор.Вставить("УдалитьСтатус",стр.УдалитьСтатус);    
		Отбор.Вставить("Период",стр.Период);
		////М = РегистрыСведений.ПО_ИсторияИзмененияСтатусов.СоздатьМенеджерЗаписи();
		////М.ДокументСсылка = ДокументСсылка;
		////М.УдалитьСтатус = Статус;
		////М.Ответственный = пПользователь;
		////М.Период = ТекущаяДата();
		////М.Записать()                       
		СК = СтрокаТаблицыЗначенийВСтруктуру(стр);
		Набор = РегистрыСведений.ПО_ИсторияИзмененияСтатусов.СоздатьНаборЗаписей();  
		Для каждого ст из Отбор Цикл  
			Набор.Отбор[ст.Ключ].Установить(ст.Значение);
		КонецЦикла;     
		
		Набор.Прочитать();  
		
		Для каждого НоваяЗапись ИЗ Набор Цикл
			
			ЗаполнитьЗначенияСвойств(НоваяЗапись,СК);        
		КонецЦикла;
		Набор.Записать(Истина);
	КонецЦикла;
	
КонецПроцедуры
