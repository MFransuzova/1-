// + ДБ Французова 25.10.2022  1595
&Вместо("ОбработкаПроведения")
Процедура ДБ_ОбработкаПроведения(Отказ, РежимПроведения)
	// + ДБ Французова 25.10.2022  1595
	Если ЭтотОбъект.ДБ_ЗабалансовыеСчета Тогда
		ДБ_ПроверкаЗаполнения(Отказ);
		Если НЕ Отказ Тогда
			Возврат;
		КонецЕсли;
		
	КонецЕсли;
	
	// - ДБ Французова 25.10.2022  1595
	ПроведениеДокументов.ОбработкаПроведенияДокумента(ЭтотОбъект, Отказ);
	
	СкладыСервер.ОтразитьСостоянияПересчетовЯчеек(ЭтотОбъект.Ссылка, ЭтотОбъект.Статус, Отказ);
	
	ПересчетТоваровЛокализация.ОбработкаПроведения(ЭтотОбъект, Отказ, РежимПроведения);
	
	ПродолжитьВызов(Отказ, РежимПроведения);
КонецПроцедуры
// - ДБ Французова 25.10.2022  1595

// + ДБ Французова 25.10.2022  1595
Процедура ДБ_ПроверкаЗаполнения(Отказ)
КонецПроцедуры
// - ДБ Французова 25.10.2022  1595
// + ДБ Французова 25.10.2022  1595
&Вместо("ОбработкаПроверкиЗаполнения")
Процедура ДБ_ОбработкаПроверкиЗаполнения(Отказ, ПроверяемыеРеквизиты)
	Если ЭтотОбъект.ДБ_ЗабалансовыеСчета Тогда
		
		ДБ_ПроверкаЗаполнения(Отказ);
		Если НЕ Отказ Тогда
			МассивНеПроверяем = Новый Массив;
			МассивНеПроверяем.Добавить("Помещение");
			МассивНеПроверяем.Добавить("Товары.Упаковка");
			МассивНеПроверяем.Добавить("Товары.Ячейка");
			МассивНеПроверяем.Добавить("ЯчейкаКонсолидацииИзлишковТоваров");
			МассивНеПроверяем.Добавить("ЯчейкаКонсолидацииИспорченныхТоваров");
			
			Для каждого эл из МассивНеПроверяем Цикл
				Найден =  ПроверяемыеРеквизиты.Найти(эл);
				Если НЕ Найден  = Неопределено Тогда
					ПроверяемыеРеквизиты.Удалить(Найден);
				КонецЕсли;
			КонецЦикла;
			Возврат;
		КонецЕсли;
	Иначе
		ПродолжитьВызов(Отказ, ПроверяемыеРеквизиты);	
	КонецЕсли;
	
КонецПроцедуры
